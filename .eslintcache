ightbox from \"simple-react-lightbox\";\nimport { SRLWrapper } from \"simple-react-lightbox\";\nimport {breakArrayIntoChunksHelper} from './helpers';\nimport { Link, Route, Switch } from \"react-router-dom\";\nimport 'semantic-ui-css/semantic.min.css';\nimport Admin from './admin/Admin';\n\nfunction App(props) {\n\n  const [navigation, setNavigation] = useState([])\n\n  useEffect(() => {\n    getNavigation()\n  },[])\n\n  function getNavigation() {\n    fetch('/navigation')\n    .then(res => res.text())\n    .then(res =>{\n      console.log(JSON.parse(res));\n      setNavigation(JSON.parse(res));\n    })\n  }\n\n  let headerDisplay = <Header navigation={navigation}/>\n  if (window.location.href.indexOf('/admin') !== -1) headerDisplay=''\n\n  return (\n    <div className=\"app\">\n      <SimpleReactLightbox>\n        {headerDisplay}\n        <SectionsContainer\n          navigation={navigation} \n        />\n      </SimpleReactLightbox>  \n    </div>\n  );\n}\n\nfunction Header(props) {\n\n  const navItemsDisplay = props.navigation.map((menuItem, index) => {\n    return (\n      <li key={index}><Link to={menuItem.nav_link}>{menuItem.title}</Link></li>\n    )\n  })\n  \n  return (\n    <header>\n      <h1>Charlotte Karlbom</h1>\n      <nav>\n        <ul>\n         {navItemsDisplay}\n        </ul>\n      </nav>\n    </header>\n  )\n}\n\nfunction SectionsContainer(props) {\n\n  const sectionsDisplay = props.navigation.map((section,index) => {\n      let sectionHtmlDisplay = (\n        <section key={index} id={section.nav_link}>\n          <h2>{section.title}</h2>\n        </section>\n      )\n      \n      switch (section.title) {\n        case 'home':\n          sectionHtmlDisplay = (\n            <Route exact path=\"/\"><GallerySection/></Route>\n          )\n          break;\n        case 'paintings':\n          sectionHtmlDisplay = (\n            <Route exact path=\"/paintings\"><PaintingsSection/></Route>\n          )\n          break;\n        case 'sculptures':\n          sectionHtmlDisplay = (\n            <Route exact path=\"/sculptures\"><SculpturesSection/></Route>\n          )\n          break;\n        case 'contact':\n          sectionHtmlDisplay = (\n            <Route exact path=\"/contact\"><ContactSection/></Route>\n          )\n          break;\n        case 'about':\n          sectionHtmlDisplay = (\n            <Route exact path=\"/about\"><AboutSection/></Route>\n          )\n          break;\n        default:\n        sectionHtmlDisplay = (\n          <Route exact path=\"/\"><GallerySection/></Route>\n        )\n          break;\n      }\n\n      return (\n        <React.Fragment key={index}>\n          {sectionHtmlDisplay}\n        </React.Fragment>\n      )\n    }\n  )\n\n  const adminSectionDisplay = (\n    <Route path=\"/admin\">\n        <Admin/>\n    </Route>\n  )\n  return(\n    <main>\n     {sectionsDisplay}\n     {adminSectionDisplay}\n    </main>\n  )\n}\n\nfunction PaintingsSection(props) {\n\n  const [paintings, setPaintings] = useState([])\n\n  useEffect(() => {\n    getPaintings()\n  },[])\n\n  function getPaintings() {\n    fetch('/pictures/paintings')\n    .then(res => res.text())\n    .then(res =>{\n      const newPaintings = breakArrayIntoChunksHelper(2, JSON.parse(res))\n      setPaintings(newPaintings);\n    })\n  }\n\n  const galleryDisplay = paintings.map((subArray, subIndex) => (\n    <div key={subIndex} className=\"container\">\n      {\n        subArray.map((painting, index) => (\n          <div key={index} className=\"box\">\n            <img src={painting.filename}/>\n            <div className=\"ui grid my-grid\">\n            <div className=\"twelve wide column twelve-wide\">\n              <h2>\n                {painting.caption}\n              </h2>\n              <p>\n                {painting.description}\n              </p>\n            </div>\n            <div className=\"four wide column four-wide\">\n              <span>\n                {painting.price + \" kr\"}\n      